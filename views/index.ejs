                                </td>
                                <td class="operation1">
                                    <div class="op">
                                        <a href="/admin/updatecase?id=<%= caseItem._id %>">
                                            <i class="fa-solid fa-file-pen"></i>
                                        </a>
                                    </div>
                                </td>
                                <td class="operation2">
                                    <div class="op" data-id="<%= caseItem._id %>">
                                        <a class="delete-op" data-id="<%= caseItem._id %>">
                                            <i class="fa-solid fa-eye"></i>
                                        </a>
                                    </div>
                                </td>
                            </tr>
                            <% }) %>
